{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fs-jira Desktop",
  "version": "0.1.0",
  "identifier": "com.fs-jira.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fs-jira Desktop",
        "width": 960,
        "height": 720,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.png"]
  }
}
